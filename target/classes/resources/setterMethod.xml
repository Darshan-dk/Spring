<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="com.xworkz.init.setter.Hotel" id="hotel"></bean>
	
	<bean class="com.xworkz.init.setter.Hotel" id ="hotelOceanPearl" ><constructor-arg value="OceanPearl"/></bean>
	
	
	<bean class="com.xworkz.init.setter.Hotel" id="hotelSetterName"><property name="place" value = "Udupi"></property></bean>
	
		<bean class="com.xworkz.init.setter.Hotel" id="hotelType"><property name="type" value = "true"></property></bean>
	
</beans>