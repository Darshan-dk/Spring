<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


<bean  class = "com.xworkz.mobile.MobileFactory" id= "brand">
<constructor-arg value="Samsung"/></bean>

<bean  class = "com.xworkz.mobile.MobileFactory" id= "price">
<constructor-arg value="15000" type="double"/></bean>



<bean class="com.xworkz.mobile.MobileFactory" id="mobileType"><property name="lteSupported" value = "true"></property></bean>
	

</beans>
