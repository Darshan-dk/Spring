<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="com.xworkz.monitor.MonitorFactory" id ="monitor"></bean>
	
	<bean class="com.xworkz.monitor.MonitorFactory" id ="constructor">
	<constructor-arg value="40"/>
	<constructor-arg value="30"/>
	</bean>
	
	<bean class="com.xworkz.monitor.MonitorFactory" id="setter">
	<property name="modelNo" value = "asdffd"></property>
	
		</bean>
	<bean class="com.xworkz.monitor.MonitorFactory" id="setterMfgDate">
	<property name="mfgDate" value = "asdffd"></property>
	</bean>
	
</beans>