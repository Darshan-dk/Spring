<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="com.xworkz.Season" id="season">
		<constructor-arg value="winter" />
	</bean>

	<bean class="com.xworkz.Season" id="temp">
		<constructor-arg value="22.15" type="float" />
	</bean>

	<bean class="com.xworkz.Season" id="noOfDays">
		<constructor-arg value="45" type="int" />
	</bean>

	<bean class="com.xworkz.Season" id="twoConstruct">
		<constructor-arg value="22.15" />
		<constructor-arg value="summer" />
	</bean>

	<bean class="com.xworkz.Season" id="twoConstructDiffArg">
		<constructor-arg value="summer" />
		<constructor-arg value="22.5" />
	</bean>
	<bean class="com.xworkz.Season" id="fourParameter">
		<constructor-arg value="summer" />
		<constructor-arg value="22.5" />
		<constructor-arg value="3" />
		<constructor-arg value="high" />
	</bean>
	

</beans>